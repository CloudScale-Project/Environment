<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

	<extension
	         id="eu.cloudscaleproject.env.projectnature"
	         point="org.eclipse.core.resources.natures">
	      <runtime>
	         <run
	               class="eu.cloudscaleproject.env.csm.diagram.wizard.CloudScaleProjectNature">
	         </run>
	      </runtime>
	 </extension>
	<extension point="org.eclipse.ui.decorators">
	<decorator
	    id="eu.cloudscaleproject.env.diagram.wizard.ProjectFolderDecorator"
	    label="CloudScale Project Folder Decorator"
	    state="true"
	    class= "eu.cloudscaleproject.env.csm.diagram.wizard.ProjectFolderDecorator"
	    adaptable="true">
	    <description>
	      CloudScale Project Folder Decorator
	    </description>
	  </decorator>
	</extension>
	 <extension
         point="org.eclipse.ui.newWizards">
         <category
               id="eu.cloudscaleproject.env.new"
               name="CloudScale Environment">
         </category>
      <wizard
            category="eu.cloudscaleproject.env.new"
            class="eu.cloudscaleproject.env.csm.diagram.wizard.CloudScaleProjectWizard"
            icon="icons/editor.png"
            id="eu.cloudscaleproject.env.csm.diagram.wizard.newCSEProjectWizard"
            name="CloudScale Project"
            project="true">
      </wizard>
      <wizard
            category="eu.cloudscaleproject.env.new"
            class="eu.cloudscaleproject.env.csm.diagram.wizard.NewScaleDLOverviewWizard"
            id="eu.cloudscaleproject.env.csm.diagram.wizard.newScaleDLOverviewWizard"
            icon="icons/editor.png"
            name="ScaleDL Overview">
      </wizard>
      <wizard
            category="eu.cloudscaleproject.env.new"
            class="eu.cloudscaleproject.env.csm.diagram.wizard.NewScaleDLOverviewWizard"
            id="eu.cloudscaleproject.env.csm.diagram.wizard.NewScaleDLArchitecturalTemplates"
            icon="icons/editor.png"
            name="ScaleDL Architectural Templates">
      </wizard>
      <wizard
            category="eu.cloudscaleproject.env.new"
            class="eu.cloudscaleproject.env.csm.diagram.wizard.NewScaleDLOverviewWizard"
            id="eu.cloudscaleproject.env.csm.diagram.wizard.NewScaleDLUsageEvolution"
            icon="icons/editor.png"
            name="ScaleDL Usage Evolution">
      </wizard>
   </extension>
   
   <extension
         point="org.eclipse.graphiti.ui.diagramTypes">
      <diagramType
            name="CSM Diagram Type"
            type="CSMDiagram"
            id="eu.cloudscaleproject.env.csm.diagram.diagramCSMType">
      </diagramType>
   </extension>
   <extension
         point="org.eclipse.graphiti.ui.diagramTypeProviders">
      <diagramTypeProvider
            name="CSM Diagram Type Provider"
            class="eu.cloudscaleproject.env.csm.diagram.diagram.CSMDiagramTypeProvider"
            id="eu.cloudscaleproject.env.csm.diagram.diagramDiagramTypeProvider">
         <diagramType
               id="eu.cloudscaleproject.env.csm.diagram.diagramCSMType">
         </diagramType>
         <imageProvider
               id="eu.cloudscaleproject.env.csm.diagram.DiagramImageProvider">
         </imageProvider>
      </diagramTypeProvider>
   </extension>
   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <content-type
            file-extensions="csm_diagram"
            id="org.eclipse.graphiti.content.diagram" 
            name="%_contenttype_diagram">
      </content-type>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      
      <editor
          class="org.eclipse.graphiti.ui.editor.DiagramEditor"
          contributorClass="org.eclipse.graphiti.ui.editor.DiagramEditorActionBarContributor"
          default="true"
          extensions="sdlo_diagram"
          icon="icons/explorer/csm_diagram.png"
          id="eu.cloudscaleproject.env.diagram.DiagramEditor"
          name="ScaleDL Overview">
       <contentTypeBinding
             contentTypeId="org.eclipse.graphiti.content.diagram">
       </contentTypeBinding>
      </editor>
      <editor
            class="org.eclipse.graphiti.ui.editor.DiagramEditor"
            contributorClass="org.eclipse.graphiti.ui.editor.DiagramEditorActionBarContributor"
            default="true"
            extensions="csm_diagram"
            icon="icons/explorer/csm_diagram.png"
            id="eu.cloudscaleproject.env.diagram.DiagramEditor"
            name="CSM">
         <contentTypeBinding
               contentTypeId="org.eclipse.graphiti.content.diagram">
         </contentTypeBinding>
      </editor>
      
      <editor
            class="eu.cloudscaleproject.env.csm.diagram.editor.platform.PlatformServiceEditor"
            id="eu.cloudscaleproject.env.csm.diagram.editor.component.ComponentEditor"
            name="Component Editor">
      </editor>
      <editor
            class="eu.cloudscaleproject.env.csm.diagram.editor.system.SystemEditor"
            id="eu.cloudscaleproject.env.csm.diagram.editor.system.SystemEditor"
            name="System Editor">
      </editor>
      <editor
            class="eu.cloudscaleproject.env.csm.diagram.editor.application.SoftwareServiceEditor"
            id="eu.cloudscaleproject.env.csm.diagram.editor.application.ApplicationServiceEditor"
            name="Application Service Editor">
      </editor>
   </extension>
   
   <extension
         point="org.eclipse.graphiti.ui.imageProviders">
      <imageProvider
            class="eu.cloudscaleproject.env.csm.diagram.DiagramImageProvider"
            id="eu.cloudscaleproject.env.csm.diagram.DiagramImageProvider">
      </imageProvider>
   </extension>
   
